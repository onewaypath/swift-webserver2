#extend("millenRoad/layout/base"):
  #export("hero_custom_css"): community team #endexport

  #export("hero_body"):
    <p class="text-white mb-4">
        One Way Path Communities brings together an inter-disciplinary team of professionals with experience in community builiding, human design, construction, information technology and administration.
    </p>
  #endexport

