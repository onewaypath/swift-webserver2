                                    <div class="team-title text-white d-flex flex-column flex-wrap">
                                        <h3>#(member.firstName) #(member.lastName)</h3>
                                        <h4 class="subtitle">#(member.position)</h4>
                                        <div class="mt-auto text-end">
                                            <button class="border-0 p-0 bg-transparent" type="button" data-bs-toggle="collapse" data-bs-target="#team#(member.id)" aria-expanded="false" aria-controls="team#(member.id)">
                                                <img src="/redesign/assets/images/Arrow-Up.svg" alt="">
                                            </button>
                                        </div>
                                    </div>
                                    <div class="collapse team-desc" id="team#(member.id)">
                                        <div class="team-descWrp">
                                            <ul>
                                                #sortedElements(member.bulletPoints)
